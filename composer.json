{
  "name": "widi/filter-specification",
  "description": "Specification pattern for filtering objects in php.",
  "authors": [
    {
      "name": "Dirk Winkhaus",
      "email": "dirkwinkhaus@googlemail.com"
    }
  ],
  "license": "LGPL-3.0-only",
  "require": {
    "php": "^8.2",
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan-phpunit": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "Widi\\Filter\\Specification\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Widi\\Filter\\Specification\\": [
        "test/Integration",
        "test/Unit"
      ]
    }
  },
  "scripts": {
    "cs-fix": "vendor/bin/php-cs-fixer fix",
    "analyse": "vendor/bin/phpstan",
    "test": "vendor/bin/phpunit"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "friendsofphp/php-cs-fixer": "^3.40",
    "phpspec/prophecy-phpunit": "^2.0"
  }
}
